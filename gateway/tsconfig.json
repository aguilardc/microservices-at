{
  "compilerOptions": {
    "module": "CommonJS",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "ES2020",
    "noImplicitAny": true,
    "strict": true,
    "moduleResolution": "Node",
    "outDir": "./cache"
  },
  "exclude": ["node_modules"],
  "include": ["./src/**/*.ts"]
}